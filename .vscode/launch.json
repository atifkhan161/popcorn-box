{
  "version": "0.2.0",
  "configurations": [

    {
      "type": "node",
      "request": "launch",
      "name": "Express server",
      "program": "${workspaceFolder}/src/server.js"
    },
    {
      "name": "Electron",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "protocol": "legacy",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
      "windows": {
        "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd"
      },
      "args": ["src/entry.js"]
    }
  ]
}
